                <FormulaParam Name="Factor" DefaultValue="1" MinValue="0" MaxValue="100" Description="" ParamType="double" />
              </Params>
            </Program>
            <Program Name="AtrDist" IsMainView="false">
              <FullName>The distance between atr sar and closing price</FullName>
