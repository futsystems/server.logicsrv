                <FormulaParam Name="NEma" DefaultValue="10" MinValue="0" MaxValue="1000" Description="" ParamType="double" />
                <FormulaParam Name="Factor" DefaultValue="1" MinValue="0" MaxValue="100" Description="" ParamType="double" />
              </Params>
            </Program>
